{
    "$schema": "https://raw.githubusercontent.com/haje01/loglab/master/schema/lab.schema.json",
    "domain": {
        "name": "boo",
        "desc": "최고의 PC 온라인 게임"
    },
    "import": ["acme3"],
    "bases": {
        "Logout": {
            "mixins": ["acme.bases.Logout"]
        }
    },
    "events": {
        "Login": {
          "mixins": ["acme.events.Login"],
          "fields": [
              {
                "name": "Platform",
                "desc": "PC의 플랫폼",
                "type": "string",
                "enum": [
                    "win", "mac", "linux"
                ]
              }
          ]
        },
        "Logout": {
            "mixins": ["bases.Logout"]
        }
    }
}
